{
  "name": "streamix",
  "version": "2.0.30",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "jasmine": "node scripts/run-jasmine.mjs",
    "jasmine:coverage": "testify --headless --browser node --coverage",
    "minify": "node terser.mjs && npx typegen",
    "clean": "eslint --config eslint.config.js --fix",
    "docs:generate": "node scripts/copy-vitepress-config.mjs && typedoc --plugin typedoc-plugin-markdown --out dist/api",
    "docs:prepare": "node scripts/docs-prepare.mjs",
    "docs:fix-api-links": "node scripts/fix-typedoc-links.mjs",
    "docs:dev": "npm run docs:prepare && npm run docs:generate && npm run docs:fix-api-links && vitepress dev dist",
    "docs:build": "npm run docs:prepare && npm run docs:generate && npm run docs:fix-api-links && vitepress build dist --base /streamix/ && npm run docs:rewrite",
    "docs:rewrite": "node scripts/rewrite-vitepress-base.mjs"
  },
  "dependencies": {
    "@angular/animations": "18.0.0",
    "@angular/common": "18.0.0",
    "@angular/compiler": "18.0.0",
    "@angular/core": "18.0.0",
    "@angular/forms": "18.0.0",
    "@angular/platform-browser": "18.0.0",
    "@angular/platform-browser-dynamic": "18.0.0",
    "@angular/router": "18.0.0",
    "tslib": "2.3.0",
    "zone.js": "0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.0.1",
    "@angular/cli": "18.0.1",
    "@angular/compiler-cli": "18.0.0",
    "@epikodelabs/testify": "file:dependencies/epikodelabs-testify-1.0.16.tgz",
    "@epikodelabs/typegen": "file:dependencies/epikodelabs-typegen-1.0.11.tgz",
    "@types/jasmine": "5.1.8",
    "@types/markdown-it": "13.0.9",
    "@types/node": "24.2.0",
    "ng-packagr": "18.0.0",
    "prettier": "3.7.4",
    "rollup": "4.52.4",
    "strip-json-comments": "5.0.3",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typedoc": "0.28.10",
    "typedoc-plugin-markdown": "4.8.1",
    "typescript": "5.4.2",
    "vitepress": "1.6.4"
  },
  "type": "module",
  "sideEffects": false
}
