{
  "name": "streamix",
  "version": "2.0.32",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "npm run jasmine",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "jasmine": "testify --headless --browser chrome",
    "jasmine:coverage": "testify --headless --browser chrome --coverage",
    "minify": "node terser.mjs && npx typegen",
    "clean": "eslint --config eslint.config.js --fix",
    "docs:generate": "node scripts/copy-vitepress-config.mjs && typedoc --plugin typedoc-plugin-markdown --out dist/api",
    "docs:prepare": "node scripts/docs-prepare.mjs",
    "docs:fix-api-links": "node scripts/fix-typedoc-links.mjs",
    "docs:dev": "npm run docs:prepare && npm run docs:generate && npm run docs:fix-api-links && vitepress dev dist",
    "docs:build": "npm run docs:prepare && npm run docs:generate && npm run docs:fix-api-links && vitepress build dist --base /streamix/ && npm run docs:rewrite",
    "docs:rewrite": "node scripts/rewrite-vitepress-base.mjs"
  },
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@epikodelabs/testify": "^1.0.16",
    "@epikodelabs/typegen": "^1.0.11",
    "tslib": "^2.8.1",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@types/jasmine": "^6.0.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^25.2.2",
    "ng-packagr": "^21.1.0",
    "prettier": "^3.8.1",
    "rollup": "^4.57.1",
    "strip-json-comments": "^5.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.10.0",
    "typescript": "^5.9.3",
    "vitepress": "^1.6.4"
  },
  "type": "module",
  "sideEffects": false
}
