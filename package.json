{
  "name": "streamix",
  "version": "2.0.28",
  "description": "Reactive data streams built on async generators.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "jasmine": "npx ts-test-runner --headless",
    "jasmine:coverage": "npx ts-test-runner --headless --coverage",
    "minify": "node terser.mjs && npx dts-bundler",
    "clean": "eslint --config eslint.config.js --fix",
    "docs:generate": "typedoc --plugin typedoc-plugin-markdown --out docs/api --hideBreadcrumbs --hidePageHeader",
    "docs:dev": "npm run docs:generate && vitepress dev docs --config vitepress.config.ts",
    "docs:build": "npm run docs:generate && vitepress build docs --config vitepress.config.ts && npm run docs:rewrite",
    "docs:rewrite": "node scripts/rewrite-vitepress-base.mjs"
  },
  "dependencies": {
    "@angular/animations": "18.0.0",
    "@angular/common": "18.0.0",
    "@angular/compiler": "18.0.0",
    "@angular/core": "18.0.0",
    "@angular/forms": "18.0.0",
    "@angular/platform-browser": "18.0.0",
    "@angular/platform-browser-dynamic": "18.0.0",
    "@angular/router": "18.0.0",
    "tslib": "2.3.0",
    "zone.js": "0.14.3"
  },
  "devDependencies": {
    "@actioncrew/dts-bundler": "file:dependencies/actioncrew-dts-bundler-1.0.11.tgz",
    "@actioncrew/ts-test-runner": "file:dependencies/actioncrew-ts-test-runner-1.0.16.tgz",
    "@angular-devkit/build-angular": "18.0.1",
    "@angular/cli": "18.0.1",
    "@angular/compiler-cli": "18.0.0",
    "@types/jasmine": "5.1.8",
    "@types/markdown-it": "13.0.9",
    "@types/node": "24.2.0",
    "jasmine": "5.9.0",
    "ng-packagr": "18.0.0",
    "prettier": "3.7.4",
    "rollup": "4.52.4",
    "strip-json-comments": "5.0.3",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typedoc": "0.28.10",
    "typedoc-plugin-markdown": "4.8.1",
    "typescript": "5.4.2",
    "vitepress": "1.6.4"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./projects/libraries/streamix/src/lib/index.ts",
    "./coroutine": "./projects/libraries/streamix/coroutine/src/lib/index.ts",
    "./http": "./projects/libraries/streamix/http/src/lib/index.ts",
    "./tracing": "./projects/libraries/streamix/tracing/src/lib/index.ts"
  }
}
